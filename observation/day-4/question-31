def fibonacci(n):
    if n <= 1:
        return n
    else:
        return fibonacci(n-1) + fibonacci(n-2)

def printFibonacciSeries(n):
    if n <= 0:
        print("Please enter a positive integer.")
    else:
        print("Fibonacci Series:")
        for i in range(n):
            print(fibonacci(i), end=" ")

n = 10
printFibonacciSeries(n)
